<fw:AcrylicWindow x:Class="WorkLogs.UI.MD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:fw="clr-namespace:SourceChord.FluentWPF;assembly=FluentWPF"
        xmlns:local="clr-namespace:WorkLogs.UI.MD"
        fw:AcrylicWindow.Enabled="True"
        fw:PointerTracker.Enabled="True"
        Background="Transparent"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="300" Width="300"
        Style="{DynamicResource WindowStyle1}">
    <Window.Resources>
        <ResourceDictionary Source="pack://application:,,,/WPF.Styles;component/WindowStyle.xaml"/>
    </Window.Resources>
    <local:ViewLogin/>
</fw:AcrylicWindow>
